        "storage",
        "cloud",
        "filesystem",
        "backup",
        "edu2work-api",
        "media"
    ],
    "homepage": "https://github.com/HoangThienToan/media",
    "license": "LGPL-3.0-or-later",
    "type": "library",
    "authors": [
