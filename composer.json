{
    "name": "edu2work/cloud",
    "version": "1.0.0",
    "description": "An SDK transfers your images to the cloud for Laravel 5|6|7|8|9.",
    "keywords": [
        "storage",
        "cloud",
        "filesystem",
        "backup",
        "edu2work-api",
        "media"
    ],
    "homepage": "https://github.com/HoangThienToan/media",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Toan Hoang",
            "email": "toanthien978@gmail.com",
            "homepage": "https://edu2work.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5 || ^7.0",
        "illuminate/cache": ">=5.1.0",
        "illuminate/container": ">=5.1.0",
        "illuminate/filesystem": ">=5.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {
            "Edu2work\\Media\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Edu2work\\Media\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0-dev"
        },
        "laravel": {
            "providers": [
                "Edu2work\\Media\\MediaEdServiceProvider"
            ],
            "aliases": {
                "DataTables": "Edu2work\\Media\\Client"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}